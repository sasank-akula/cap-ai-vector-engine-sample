{
  "name": "cap-ai-vector-engine-example",
  "version": "1.0.0",
  "description": "A CAP & AI Example using the CAP LLM Plugin.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1.9.1",
    "@sap-cloud-sdk/http-client": "^3.14.0",
    "@sap/cds": "^8.9.4",
    "@sap/cds-hana": "^2.1.0",
    "@sap/xssec": "^4.9.0",
    "cap-llm-plugin": "^1.3.1",
    "express": "^4",
    "langchain": "^0.1.36"
  },
  "devDependencies": {
    "@cap-js/sqlite": "1.7.3",
    "@sap/cds-dk": "8.0.2"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "auth": "dummy",
      "db": "hana",
      "GENERATIVE_AI_HUB": {
        "gpt-4": {
          "destinationName": "GenAIHubDestination",
          "deploymentUrl": "/inference/deployments/d07c950f24f4b589",
          "resourceGroup": "default",
          "apiVersion": "2023-05-15",
          "modelName": "gpt-4"
        },
        "text-embedding-ada-002": {
          "destinationName": "GenAIHubDestination",
          "deploymentUrl": "/inference/deployments/d0b11b39cbc4af5e",
          "resourceGroup": "default",
          "apiVersion": "2023-05-15",
          "modelName": "text-embedding-ada-002"
        }
      },
      "GenAIHubDestination": {
        "kind": "rest",
        "credentials": {
          "destination": "GENERATIVE_AI_HUB",
          "requestTimeout": "300000"
        }
      }
    }
  }
}
